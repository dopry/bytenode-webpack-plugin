{
  "name": "@dopry/bytenode-webpack-plugin",
  "description": "Compile JavaScript into bytecode using bytenode",
  "version": "1.2.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "github:dopry/bytenode-webpack-plugin"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "all": "npm-run-all",
    "build": "tsc --build tsconfig.project.json",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf lib **/.eslintcache **/*.log **/*.tsbuildinfo",
    "lint": "eslint . --ext .js,.ts --cache --max-warnings 0",
    "release": "semantic-release",
    "test": "jest --config jest.config.ts --detectOpenHandles",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies_comments": {
    "bytenode": "pinned at 1.3.4 because electron is not found in 1.3.5 and 1.3.6, see: https://github.com/bytenode/bytenode/issues/180"
  },
  "dependencies": {
    "bytenode": "1.3.4",
    "slash": "3.0.0"
  },
  "peerDependencies": {
    "webpack": ">= 5.0.0"
  },
  "devDependencies": {
    "@jest/types": "26.6.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/jest": "26.0.22",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "eslint": "^8.31.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.2",
    "ts-jest": "26.5.4",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "5.7.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/herberttn/bytenode-webpack-plugin",
  "bugs": "https://github.com/herberttn/bytenode-webpack-plugin/issues",
  "keywords": [
    "bytecode",
    "bytenode",
    "compile",
    "javascript",
    "js",
    "node",
    "nodejs",
    "plugin",
    "webpack"
  ],
  "author": {
    "name": "herberttn",
    "url": "https://github.com/herberttn"
  },
  "contributors": [
    {
      "name": "jjeff",
      "url": "https://github.com/jjeff"
    },
    {
      "name": "Darrel O'Pry",
      "url": "https://github.com/dopry"
    }
  ]
}
